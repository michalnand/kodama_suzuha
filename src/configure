#/bin/bash

#device=motoko
device=aeris
#device=yrobot

echo configuring for device $device



if [ "$device" == "motoko" ] ; then
	cp makefiles/Makefile_motoko Makefile
fi

if [ "$device" == "aeris" ] ; then
	cp makefiles/Makefile_aeris Makefile
fi

if [ "$device" == "yrobot" ] ; then
	cp makefiles/Makefile_yrobot Makefile
fi
